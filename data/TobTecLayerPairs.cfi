		       "TEC1_pos+TEC2_neg", "TEC1_pos+TEC3_neg", "TEC2_neg+TEC3_neg", "TEC2_neg+TEC4_neg", "TEC3_neg+TEC4_neg", "TEC3_neg+TEC5_neg", "TEC4_neg+TEC5_neg", "TEC4_neg+TEC6_neg", "TEC5_neg+TEC6_neg", "TEC5_neg+TEC7_neg", "TEC6_neg+TEC7_neg"
