		       "TEC1_pos+TEC2_pos", "TEC1_pos+TEC3_pos", "TEC2_pos+TEC3_pos", "TEC2_pos+TEC4_pos", "TEC3_pos+TEC4_pos", "TEC3_pos+TEC5_pos", "TEC4_pos+TEC5_pos", "TEC4_pos+TEC6_pos", "TEC5_pos+TEC6_pos", "TEC5_pos+TEC7_pos", "TEC6_pos+TEC7_pos",
